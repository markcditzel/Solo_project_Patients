<h1> Patients </h1>

<table>
  <tr>
    <th>First Name</th>
    <th></th>
    <th>Secon Name</th>
  </tr>
  <% for patient in @patients %>
  <tr>
    <td><a href="/patients/<%=patient.id %>"><%= patient.first_name%></a></td>
    <td>|</td>
    <td><%= patient.second_name %></td>
  </tr>
  <% end %>
</table>
